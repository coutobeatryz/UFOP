#include <stdio.h>
#include <stdlib.h>

// Construa um programa que imprima todas as possíveis jogadas para quatro dados normais,
// com faces de 1 a 6, sem que haja, em cada jogada, repetição de valores para dados distintos.

int main(){
 int i, j;

 for(i=1; i<=6; ++i){
    for(j=1; j<=i; ++j){
        printf("(%d,%d)\n", i, j);
    }
 }

return 0;
}
